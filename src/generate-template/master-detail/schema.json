{
  "$schema": "http://json-schema.org/schema",
  "id": "SchematicsMasterDetail",
  "title": "Styling Options Schema",
  "type": "object",
  "properties": {
    "master": {
      "type": "string",
      "description": "The name of the master component."
    },
    "detail": {
      "type": "string",
      "description": "The name of the detail component."
    },
    "project": {
      "type": "string",
      "description": "The name of the project.",
      "$default": {
        "$source": "projectName"
      }
    }
  },
  "required": [
    "master",
    "detail"
  ]
}
 
